<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

  <!--light Mode-->
  <Color x:Key="GreenColor">green</Color>
  <Color x:Key="WhiteColor">#ffffff</Color>
  <Color x:Key="BlackColor">#000000</Color>
  <Color x:Key="OrangeColor">orange</Color>
  <Color x:Key="GrayColor">gray</Color>
  <Color x:Key="ToreaBayColor">#14139A</Color>
  <Color x:Key="PurpleColor">Purple</Color>
  <Color x:Key="RedColor">red</Color>
  <Color x:Key="SkyBlueColor">SkyBlue</Color>

  <!--dark Mode-->
  <Color x:Key="DarkGreenColor">green</Color>
  <Color x:Key="DarkWhiteColor">#ffffff</Color>
  <Color x:Key="DarkBlackColor">#000000</Color>
  <Color x:Key="DarkOrangeColor">orange</Color>
  <Color x:Key="DarkGrayColor">gray</Color>
  <Color x:Key="DarkToreaBayColor">#14139A</Color>
  <Color x:Key="DarkPurpleColor">Purple</Color>
   <Color x:Key="DarkRedColor">red</Color>
   <Color x:Key="DarkSkyBlueColor">SkyBlue</Color>
</ResourceDictionary>

